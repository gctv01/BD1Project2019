<div class="label text-center my-2 display-1">EMPLEADOS</div>
<div class="m-2 bg-white p-1">
    <table id="example" class="table table-striped table-bordered table-hover" style="width:100%">
        <thead>
            <tr>
                <th>Primer Nombre</th>
                <th>Segundo Nombre</th>
                <th>Primer Apellido</th>
                <th>Segundo Apellido</th>
                <th>DI</th>
                <th>Fecha de nacimiento</th>
                <th>Genero</th>
                <th>Tipo de sangre</th>
                <th>Titulo</th>
                <th>DI supervisor</th>
            </tr>
        </thead>
        <tbody>
            {{#each empleado}}
            <tr>
                <td>{{nombre}}</td>
                <td>{{nombre2}}</td>
                <td>{{apellido}}</td>
                <td>{{apellido2}}</td>
                <td>{{di}}</td>
                <td>{{fecha_nacimiento}}</td>
                <td>{{genero}}</td>
                <td>{{tipo_sangre}}</td>
                <td>{{titulo}}</td>
                <td>{{di_supervisor}}</td>
                <form action="/empleado/eliminar{{di}}" method="GET">
                    <td><button class="btn btn-danger">eliminar</button></td>
                </form>
                <form action="/empleado/modificar{{di}}" method="GET">                   
                    <td><button class="btn btn-warning">modificar</button></td>
                </form>
            </tr>
            {{/each}}
        </tbody>
        <tfoot>
            <tr>
                <th>Primer Nombre</th>
                <th>Segundo Nombre</th>
                <th>Primer Apellido</th>
                <th>Segundo Apellido</th>
                <th>DI</th>
                <th>Fecha de nacimiento</th>
                <th>Genero</th>
                <th>Tipo de sangre</th>
                <th>Titulo</th>
                <th>DI supervisor</th>
            </tr>
        </tfoot>
    </table>
</div>